    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^8.2.0",
    "csstype": "^3.2.3",
    "electron": "^33.0.0",
    "electron-builder": "^25.0.0",
    "typescript": "^5.5.0",
    "vite": "^5.4.21",
